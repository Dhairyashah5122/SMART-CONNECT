  "extraction_patterns": {
    "section_headers": [
      "skills", "technical skills", "core competencies", "expertise", "proficiencies",
      "technologies", "programming languages", "tools", "software", "platforms",
      "languages", "certifications", "qualifications", "abilities", "experience with"
    ],
    "skill_indicators": [
      "experience in", "proficient in", "skilled in", "expertise in", "knowledge of",
      "familiar with", "worked with", "used", "developed with", "built using",
      "implemented in", "created with", "years of experience", "competent in"
    ],
    "context_keywords": [
      "project", "developed", "created", "built", "implemented", "designed", "worked on",
      "responsible for", "led", "managed", "collaborated", "contributed", "maintained",
      "optimized", "integrated", "deployed", "tested", "debugged", "troubleshot"
    ]
  },
  "scoring_algorithm": {
    "base_weight": 1.0,
    "frequency_multiplier": 0.1,
    "context_bonus": 0.2,
    "section_bonus": 0.3,
    "experience_multiplier": {
      "beginner": 0.5,
      "intermediate": 1.0,
      "advanced": 1.5,
      "expert": 2.0
    },
    "recency_decay": {
      "current": 1.0,
      "1_year": 0.9,
      "2_years": 0.8,
      "3_years": 0.7,
      "4_years": 0.6,
      "5_years": 0.5
    }
  },
  "confidence_thresholds": {
    "high": 0.8,
    "medium": 0.6,
    "low": 0.4,
    "minimum": 0.2
  },
  "output_format": {
    "skills": [
      {
        "name": "string",
        "category": "string",
        "confidence": "float",
        "experience_level": "string",
        "context": ["string"],
        "frequency": "integer",
        "weight": "float"
      }
    ],
    "summary": {
      "total_skills": "integer",
      "high_confidence_skills": "integer",
      "top_categories": ["string"],
      "overall_score": "float",
      "recommendations": ["string"]
    }
  },
  "ai_prompts": {
    "skill_extraction": {
      "system_prompt": "You are an expert at extracting technical and professional skills from resumes and job applications. Analyze the provided text and identify all relevant skills, categorize them appropriately, and assess the candidate's proficiency level based on context clues.",
      "user_prompt_template": "Extract all skills from the following resume/application text. For each skill, provide:\n1. Skill name (normalized)\n2. Category (from predefined categories)\n3. Confidence level (0.0-1.0)\n4. Experience level (Beginner/Intermediate/Advanced/Expert)\n5. Context where mentioned\n\nText: {resume_text}\n\nRespond with a structured JSON format as specified in the configuration."
    },
    "skill_normalization": {
      "system_prompt": "You are an expert at normalizing and standardizing technical skill names. Convert variations and synonyms to their canonical forms.",
      "user_prompt_template": "Normalize these skill names to their standard forms: {skill_list}"
    },
    "experience_assessment": {
      "system_prompt": "You are an expert at assessing technical experience levels based on context and project descriptions.",
      "user_prompt_template": "Based on this context: '{context}', assess the experience level for {skill_name}. Consider project complexity, responsibilities, and duration."
    }
  },
  "validation_rules": {
    "minimum_skill_length": 2,
    "maximum_skill_length": 50,
    "minimum_confidence": 0.2,
    "maximum_skills_per_resume": 100,
    "required_categories": ["programming_languages", "web_technologies", "soft_skills"]
  },
  "processing_options": {
    "remove_duplicates": true,
    "normalize_case": true,
    "merge_similar_skills": true,
    "similarity_threshold": 0.8,
    "include_soft_skills": true,
    "include_certifications": true,
    "language_detection": true,
    "supported_languages": ["en", "es", "fr", "de", "it", "pt"]
  }
}